<img src="blank.png" width="15" id="q6bi"/><input type="radio" name="q6" value="b" id="q6b"><label for="q6b">$\{49,41,30\}$</label><br>          
<img src="blank.png" width="15" id="q6ci"/><input type="radio" name="q6" value="c" id="q6c"><label for="q6c">$\{45,46,29\}$</label><br>        
<img src="blank.png" width="15" id="q6di"/><input type="radio" name="q6" value="d" id="q6d"><label for="q6d">$\{45,29,46\}$</label><br>
    </ul>
    
<script>
  document.getElementById('q6a').checked = false;
